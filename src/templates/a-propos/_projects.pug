mixin item(text, link, linkText, date, linkLang, textLang)
	li
		span(lang=textLang)=text + ' - '
		if link
			a(href=link, lang=linkLang)=linkText
			| &nbsp;-&nbsp;
		else if linkText
			=linkText + ' - '
		=date

